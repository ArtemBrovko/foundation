<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="modera_backend_dashboard.contributions.menu_items_provider" class="Modera\BackendDashboardBundle\Contributions\MenuItemsProvider">
            <tag name="mf.jsruntimeintegration.menu.menu_items_provider" />
        </service>

        <service id="modera_backend_dashboard.contributions.dashboard_provider" class="Modera\BackendDashboardBundle\Contributions\DashboardProvider">
            <tag name="modera_backend_dashboard.dashboard_provider" />
        </service>

        <service id="modera_backend_dashboard.dashboard_service" class="Modera\BackendDashboardBundle\Dashboard\DashboardService">
            <argument type="service" id="modera_backend_dashboard.dashboard_provider" />
        </service>

        <service id="modera_backend_dashboard.contributions.config_mergers_provider"
                 class="Modera\BackendDashboardBundle\Contributions\ConfigMergersProvider">

            <argument type="service" id="service_container" />
            <argument type="service" id="modera_backend_dashboard.dashboard_provider" />

            <tag name="mf.jsruntimeintegration.config.config_mergers_provider" />
        </service>

        <service id="modera_backend_dashboard.contributions.client_di_service_definitions_provider"
                 class="Modera\BackendDashboardBundle\Contributions\ClientDiServiceDefinitionsProvider">

            <tag name="modera_security_aware_js_runtime.client_di_service_defs_provider" />
        </service>
    </services>
</container>